{% extends "::base.html.twig" %}

{% block title %}AppBundle:Heroku:my{% endblock %}

{% block body %}
<h1>Welcome to the Heroku:my page</h1>
 <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Firstname</th>
                <th>Lastname</th>
                <th>Email</th>
                <th>Mobilephone</th>
              
            </tr>
        </thead>
        <tbody>
        {% for result in results %}
            <tr>
                <td>{{ result.id }}</a></td>
                <td>{{ result.name }}</td>
                <td>{{ result.firstName }}</td>
                <td>{{ result.lastName }}</td>
                <td>{{ result.email }}</td>
                <td>{{ result.mobilePhone }}</td>
                                         </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
